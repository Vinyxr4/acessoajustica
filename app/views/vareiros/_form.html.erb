<%= form_for @vareiro, :html => { :class => "form-horizontal vareiro" } do |f| %>
    
  <%= render :partial => 'membros/fields', :locals => { :f => f } %>
  <%= render :partial => 'fields', :locals => { :f => f } %>
  <%= f.submit nil, :class => 'btn btn-primary' %>
  <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
      vareiros_path, :class => 'btn btn-default' %>

<% end %>
